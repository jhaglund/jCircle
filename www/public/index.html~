<script>
(function($){
  $.fn.jCircle = function( callback ) {
    var previousDegree = 0; //start at the top
    circleCheck = function(){}
    if(isACircle){
      callback();
    }
    return this;
  };
}(jQuery));

//include jCircle-01.js
var callback = function(direction, degree, velocity)
{
  console.log(direction, degree, velocity);
  return;
};
//when a circular UI event starts in the #circular1 div, callback will be triggered
$.jCircle('#circular1', callback);

</script>
